<?xml version='1.0'?>
<sdf version="1.9">
  <model name="iris_with_ardupilot_with_sensor">
    
        <link name='lidar_link'>
      <pose relative_to='lidar_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.15</mass>
        <inertia>
          <ixx>9.09375e-05</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>9.09375e-05</iyy>
          <iyz>0</iyz>
          <izz>9.1875e-05</izz>
        </inertia>
      </inertial>
      <collision name='collision'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <cylinder>
            <length>0.06</length>
            <radius>0.035</radius>
          </cylinder>
        </geometry>
      </collision>

      <sensor name='lidar' type='gpu_lidar'>
        <always_on>1</always_on>
        <update_rate>5</update_rate>
        <visualize>1</visualize>
        <topic>lidar_link/sensor/lidar</topic>
        <pose>0 0 0.045 0 -0 0</pose>
        <ray>
          <scan>
            <horizontal>
              <samples>360</samples>
              <resolution>1</resolution>
              <min_angle>0</min_angle>
              <max_angle>6.28319</max_angle>
            </horizontal>
            <vertical>
              <samples>1</samples>
              <resolution>1</resolution>
              <min_angle>0</min_angle>
              <max_angle>0</max_angle>
            </vertical>
          </scan>
          <range>
            <min>0.1</min>
            <max>12</max>
            <resolution>0.05</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
      </sensor>
    </link>

    <joint name='lidar_joint' type='revolute'>
      <pose relative_to='base_link'>0.11 0 0.03 0 0 0</pose>
      <parent>base_link</parent>
      <child>lidar_link</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>0</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <damping>1.0</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

    <link name='sonar_front_link'>
      <pose relative_to='sonar_front_joint'>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.02</mass>
        <inertia>
          <ixx>3.04167e-06</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>7.5e-07</iyy>
          <iyz>0</iyz>
          <izz>3.04167e-06</izz>
        </inertia>
      </inertial>
      <collision name='collision'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.015 0.04 0.015</size>
          </box>
        </geometry>
      </collision>
		
      <sensor name='sonar' type='gpu_lidar'>
        <always_on>1</always_on>
        <update_rate>20</update_rate>
        <visualize>1</visualize>
        <topic>sonar_front_link/sensor/sonar</topic>
        <pose>0 0 0 0 0 0</pose>
        <ray>
          <scan>
            <horizontal>
              <samples>5</samples>
              <resolution>1</resolution>
              <min_angle>-0.14835</min_angle>
              <max_angle>0.14835</max_angle>
            </horizontal>
            <vertical>
              <samples>3</samples>
              <resolution>1</resolution>
              <min_angle>0</min_angle>
              <max_angle>0.14835</max_angle>
            </vertical>
          </scan>
          <range>
            <min>0.01</min>
            <max>4</max>
            <resolution>0.02</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.1</mean>
            <stddev>0.005</stddev>
          </noise>
        </ray>
      </sensor>
    </link>

    <joint name='sonar_front_joint' type='revolute'>
      <pose relative_to='base_link'>0.225 0 0.01 0 0 0</pose>
      <parent>base_link</parent>
      <child>sonar_front_link</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>0</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <damping>1.0</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

    
    
    <include>
      <uri>model://iris_with_standoffs</uri>
    </include>

    <!--
      visual markers for debugging
      - blade root    - teal
      - blade tip     - orange
      - blade cp      - yellow
      - blade forward - red
      - blade upward  - blue
    -->

    
    
