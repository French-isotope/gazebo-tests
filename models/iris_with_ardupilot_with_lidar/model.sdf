      </control>
          </plugin>



      <!-- 
        RNGFND1_TYPE = 100 (SITL)
        RNGFND1_FUNCTION = 0 (LINEAR)
        RNGFND1_MIN_CM = 10 (0.1 m)
        RNGFND1_MAX_CM = 400 (4.0 m)
        RNGFND1_POS_X = 0.25
        RNGFND1_POS_Y = 0.0
        RNGFND1_POS_Z = -0.05
        RNGFND1_ORIENT = 0 (Forward)
      -->
      <sensor>
        <type>sonar</type>
        <index>1</index>
        <topic>sonar_front_link/sensor/sonar</topic>
      </sensor>



